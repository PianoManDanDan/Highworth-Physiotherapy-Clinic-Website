{
  "name": "highworth-physio-clinic-next-app",
  "version": "0.1.0",
  "author": "Daniel Martin",
  "description": "NextJS app for Highworth Physiotherapy Clinic Website",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next build && next start",
    "export": "next build && next export",
    "test": "jest --config tests/config/jest.config.js",
    "test:watch": "jest --watch --config tests/config/jest.config.js",
    "jest:coverage": "jest --collect-coverage --config tests/config/jest.config.js",
    "test:update-snapshot": "jest --update-snapshot --config tests/config/jest.config.js",
    "test:ci": "jest --ci --reporters=default --config tests/config/jest.config.js",
    "storybook": "start-storybook -s ./public -p 6006",
    "generate": "plop"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "next": "^10.1.3",
    "next-themes": "0.0.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@storybook/addon-actions": "^6.2.7",
    "@storybook/addon-essentials": "^6.2.7",
    "@storybook/addon-links": "^6.2.7",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.2.7",
    "@testing-library/dom": "^7.30.3",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.3",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.4",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "next-page-tester": "^0.24.2",
    "plop": "^2.7.4",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "storybook-addon-next-router": "^2.0.4",
    "storybook-addon-pseudo-states": "^1.0.0",
    "style-loader": "^2.0.0",
    "typescript": "^4.2.4"
  }
}
